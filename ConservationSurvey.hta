<META HTTP-EQUIV="MSThemeCompatible" CONTENT="YES">
<script language="VBScript">
Option Explicit
Dim Title : Title = "Conservation Survey Generator"
'**********************************************************************************************
Sub Window_OnLoad
	MsgBox "Welcome to the Newfields Conservation Survey Generator!"& vbCrLf & vbCrLf &"Instructions:"& vbCrLf & vbCrLf &"- To generate a Conservation Survey for a specific Clowes object, select the Clowes Number (or accession number) from the drop-down list and click the 'Generate Survey(s)' button."& vbCrLf & vbCrLf &" - To generate all Clowes Conservation Surveys at once, select 'ALL' in the drop-down menu and click the 'Generate Survey(s)' button. This will result in all the Clowes Conservation Surveys presented in a single file, with page breaks between reports."& vbCrLf & vbCrLf &"- If there are no errors, the resulting report should open in your browser of choice. From there, you can print to PDF and save in your location of choice."& vbCrLf & vbCrLf & vbCrLf &"Reporting errors:"& vbCrLf & vbCrLf &"If the resulting report does not look correct, you receive an error message, or if your browser fails to automatically open the report, follow the instructions in the 'Processing Report' section to report the issue to the Newfields Lab.",,"Welcome and Instructions"
End Sub
'********************************************************************************************** 
Sub instructions_onclick 
	MsgBox "Instructions:"& vbCrLf & vbCrLf &"- To generate a Conservation Survey for a specific Clowes object, select the Clowes Number (or accession number) from the drop-down list and click the 'Generate Survey(s)' button."& vbCrLf & vbCrLf &" - To generate all Clowes Conservation Surveys at once, select 'ALL' in the drop-down menu and click the 'Generate Survey(s)' button. This will result in all the Clowes Conservation Surveys presented in a single file, with page breaks between reports."& vbCrLf & vbCrLf &"- If there are no errors, the resulting report should open in your browser of choice. From there, you can print to PDF and save in your location of choice."& vbCrLf & vbCrLf & vbCrLf &"Reporting errors:"& vbCrLf & vbCrLf &"If the resulting report does not look correct, you receive an error message, or if your browser fails to automatically open the report, follow the instructions in the 'Processing Report' section to report the issue to the Newfields Lab.",,"Instructions"
End Sub 
'**********************************************************************************************
Sub Run_Cmd(strCommand)
On Error Resume Next
    Output.value = ""
    btnClick.disabled = True
    document.body.style.cursor = "wait"
    btnClick.style.cursor = "wait"
    Const ForReading = 1
    Const TristateTrue = -1
    Const TemporaryFolder = 2
    Const WshHide = 0
    Dim wsh, fs, ts
    Dim strTempFile,strFile, strData
    Set wsh = CreateObject("Wscript.Shell")
    Set fs = CreateObject("Scripting.FileSystemObject")
    strTempFile = fs.BuildPath(fs.GetSpecialFolder(TemporaryFolder).Path, fs.GetTempName)
    strFile = fs.BuildPath(fs.GetSpecialFolder(TemporaryFolder).Path, "result.txt")
    wsh.Run "cmd.exe /c " & strCommand & " > " & DblQuote(strTempFile) & "2>&1", WshHide, True
    wsh.Run "cmd.exe /u /c Type " & DblQuote(strTempFile) & " > " & DblQuote(strFile) & "", WshHide, True
    Set ts = fs.OpenTextFile(strFile,ForReading,True,TristateTrue)
    strData = ts.ReadAll
    Output.Value = "Microsoft Windows [version 7.1 7631]" & vbcrlf &_
    "Copyright (c) 2009 Microsoft Corporation. All rights reserved." & vbcrlf & vbcrlf &_
    "C:\>"& strCommand & vbcrlf & strData
    ts.Close
    fs.DeleteFile strTempFile
    fs.DeleteFile strFile
    document.body.style.cursor = "default"
    btnClick.style.cursor = "default"
    btnClick.disabled = False   
End Sub
'**********************************************************************************************
Function DblQuote(Str)
    DblQuote = Chr(34) & Str & Chr(34)
End Function
'**********************************************************************************************
Sub OnClickButtonCopy()
    document.parentwindow.clipboardData.SetData "text", Output.Value
    MsgBox "The Processing Report has been copied to the clipboard.",vbInformation,Title
End Sub
'**********************************************************************************************
</script>
